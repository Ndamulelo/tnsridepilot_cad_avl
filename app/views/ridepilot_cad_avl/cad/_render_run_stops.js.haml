-# Marking stops on the map
- unless @itins_data.blank?
  var run_ids = #{(@run_ids || []).to_json.html_safe};
  run_ids.forEach(function(run_id) { removeRunStops(run_id); });
  
  var itins_data = #{@itins_data.to_json.html_safe};
  var idx = 0;
  itins_data.forEach(function(itin_data) { 
  var seq_number = null;
  var itin_type = itin_data[4];
  // TODO: Work on the code for marking the last stop on the map
  if(itin_type == 1) {
  idx ++;
  seq_number = idx;
  drawRunStop(itin_data, seq_number);
  }
   
  });